L ->
  tinuso.md:	(match_operand:SI 1 "constant_operand"       "L,i"))]
  constraints.md:(define_constraint "L"
P ->
  tinuso.md:	(match_operand:SI 1 "constant_P_operand"       "P"))]
  constraints.md:(define_constraint "P"
I ->
  tinuso.md:		   (match_operand:SI 2 "immediate_operand" "I")))]
  tinuso.md:;;		   (match_operand:SI 2 "immediate_operand" "I")))]
  tinuso.md:;;		   (match_operand:SI 2 "immediate_operand" "I")))]
  tinuso.md:		     (match_operand:SI 2 "nonmemory_operand" "I")))]
  tinuso.md:;;		     (match_operand:SI 2 "nonmemory_operand" "I")))]
  tinuso.md:;;		     (match_operand:SI 2 "nonmemory_operand" "I")))]
  tinuso.md:		     (match_operand:SI 2 "nonmemory_operand" "I")))]
  tinuso.md:;;		   (match_operand:SI 2 "immediate_operand" "I")))]
  tinuso.md:;;		   (match_operand:SI 2 "immediate_operand" "I")))]
  constraints.md:(define_constraint "I"
J ->
  tinuso.md:	(plus:SI (match_operand:SI 1 "register_or_zero_operand" "%rJ,rJ")
  tinuso.md:	(minus:SI (match_operand:SI 1 "register_or_zero_operand" "rJ,rJ")
  tinuso.md:               (eq:SI (match_operand:SI 1 "register_or_zero_operand" "rJ")
  tinuso.md:                      (match_operand:SI 2 "register_or_zero_operand" "rJ")))]
  tinuso.md:               (ne:SI (match_operand:SI 1 "register_or_zero_operand" "rJ")
  tinuso.md:                      (match_operand:SI 2 "register_or_zero_operand" "rJ")))]
  tinuso.md:        (gt:SI (match_operand:SI 1 "register_or_zero_operand" "rJ")
  tinuso.md:               (match_operand:SI 2 "register_or_zero_operand" "rJ")))]
  tinuso.md:        (gtu:SI (match_operand:SI 1 "register_or_zero_operand" "rJ")
  tinuso.md:                (match_operand:SI 2 "register_or_zero_operand" "rJ")))]
  tinuso.md:        (lt:SI (match_operand:SI 1 "register_or_zero_operand" "rJ")
  tinuso.md:                (match_operand:SI 2 "register_or_zero_operand" "rJ")))]
  tinuso.md:        (ltu:SI (match_operand:SI 1 "register_or_zero_operand" "rJ")
  tinuso.md:                (match_operand:SI 2 "register_or_zero_operand" "rJ")))]
  tinuso.md:        (ge:SI (match_operand:SI 1 "register_or_zero_operand" "rJ")
  tinuso.md:                (match_operand:SI 2 "register_or_zero_operand" "rJ")))]
  tinuso.md:        (geu:SI (match_operand:SI 1 "register_or_zero_operand" "rJ")
  tinuso.md:                (match_operand:SI 2 "register_or_zero_operand" "rJ")))]
  tinuso.md:        (le:SI (match_operand:SI 1 "register_or_zero_operand" "rJ")
  tinuso.md:               (match_operand:SI 2 "register_or_zero_operand" "rJ")))]
  tinuso.md:        (leu:SI (match_operand:SI 1 "register_or_zero_operand" "rJ")
  tinuso.md:                (match_operand:SI 2 "register_or_zero_operand" "rJ")))]
  tinuso.md:	(if_then_else (eq:SI (match_operand:SI 0 "register_or_zero_operand" "r,J")
  tinuso.md:	(if_then_else (ne:SI (match_operand:SI 0 "register_or_zero_operand" "r,J")
  constraints.md:(define_constraint "J"
K ->
  predicates.md:(define_predicate "constant_K_operand"
  predicates.md:       (match_test "satisfies_constraint_K (op)")))
  predicates.md:(define_predicate "register_or_K_operand"
  predicates.md:       (match_operand 0 "constant_K_operand")))
  predicates.md:	  && (satisfies_constraint_K (op)
M ->
predicates.md:	      || satisfies_constraint_M (op)
