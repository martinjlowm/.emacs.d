function T = convertTemperature(T, unitFrom, unitTo)
keys = {'Celcius', 'Fahrenheit', 'Kelvin'}

celciusValues =    [sym('T'),              sym('1.8 * T + 32'),     sym('T + 273.15')];
fahrenheitValues = [sym('(T - 32) / 1.8'), sym('T'),                sym('(T + 459.67) / 1.8')];
kelvinValues =     [sym('T - 273.15'),     sym('1.8 * T - 459.67'), sym('T')];

celciusMap = containers.Map(keys, celciusValues);
fahrenheitMap = containers.Map(keys, fahrenheitValues);
kelvinMap = containers.Map(keys, kelvinValues);

conversionExpressions = containers.Map(keys, [celciusMap, fahrenheitMap, kelvinMap]);
row = conversionExpressions('Celcius');
row('Celcius')
end